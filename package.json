    "single-spa": "^4.4.1",
    "single-spa-vue": "^1.5.4",
    "systemjs": "^6.7.1",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
